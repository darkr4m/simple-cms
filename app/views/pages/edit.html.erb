<%= link_to("<< Back to list", pages_path, :class => 'back-link') %>

<div class="pages edit">
  <h2>Edit Page</h2>
  <%= form_for(@page, :url => page_path(@page), :method => 'patch') do |f| %>
  <%= render(:partial => 'form', :locals => {:f => f}) %>
    <div class="form-buttons">
      <%= f.submit("Edit Page") %>
    </div>
  <% end %>
</div>
